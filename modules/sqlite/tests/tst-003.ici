#!/usr/local/bin/path
sys.access(tstdb.NAME);
db = sqlite.open(tstdb.NAME);

for (i = 0; i < tstdb.V1; ++i)
{
    for (j = 0; j < tstdb.V2; ++j)
    {
	for (k = 0; k < tstdb.V3; ++k)
	{
	    id := tstdb.id(i, j, k);
	    rows := db:exec(sprintf("select * from %s where id = %d", tstdb.TABLE, id));
	}
    }
}
