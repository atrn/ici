local Z = 3 * 60 * int(44100) * 2;
local N = 100;

local v0 = vec32(Z, 1.0);
local v1 = vec32(Z);
local v2 = vec32(Z);

vec.randomize(v1);
vec.randomize(v2);

elapsed := util.measure_elapsed_time
(
    [func () {
        for (i := 0; i < N; ++i)
        {
            v3 := v0 + v1 * v2;
        }
    }]
);
per := elapsed / N;
printf("%d elements, %d times \"v0 + v1 * v2\"  ->  %s  ->  %s/op\n", v0.size, N, pretty.time(elapsed, 2), pretty.time(per, 4));
