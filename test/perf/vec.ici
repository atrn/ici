local Z = 240 * int(44100);
local N = 100;

local v0 = vec32(Z, 1.0);
local v1 = vec32(Z);
local v2 = vec32(Z);

noise(v1);
noise(v2);

elapsed := util.measure_elapsed_time
(
    [func () {
        for (i := 0; i < N; ++i)
        {
            v3 := v0 + v1 * v2;
        }
    }]
);

printf("%d elements, ", v0.size);
caption := sprintf("%d times \"v0 + v1 * v2\"", N),
printf("%s  ->  %.06fs  ->  ", caption, elapsed);
per := elapsed / N;
printf("%.03fus/op\n", per*1e6);
